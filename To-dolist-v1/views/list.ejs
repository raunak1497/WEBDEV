 <%- include("header") -%>
 
 <div class="box" id="heading">
      <h1> <%= listTitle %> </h1>
    </div>
    
    <div class="box">
      
      <ul>
        <% for(var i =0; i< newListItem.length ; i++) { %>
          <div class="item">
            <input type="checkbox">
            <p> <%= newListItem[i] %></p>
          </div>
          
        <% } %>
      </ul>

      <form action="/" class="item" method="POST">
        <input type="text" name="newItem" class="" placeholder="New item" autocomplete="off">
        <button type="submit" name="list" value=<%= listTitle %> >+</button>
      </form>

    </div>

    <%- include("footer") -%>
